.Wdate{
	border:#ccc 1px solid;
	height:30px;
	line-height: 30px;
	background:#fff url(datePicker.gif) no-repeat right;
	border-radius: 2px;
	text-indent: 1em;
	box-shadow: 0px 1px 5px rgba(0,0,0,.05) inset;
}
.Wdate::-ms-clear{display:none;}

.WdateFmtErr{
	font-weight:bold;
	color:red;
}